{
  "name": "create-bi-config",
  "version": "0.2.2",
  "type": "module",
  "license": "MIT",
  "author": "Boehringer Ingelheim",
  "description": "Add shared configs to your project.",
  "keywords": [
    "boehringer",
    "boehringer-ingelheim",
    "configuration",
    "editorconfig",
    "prettier",
    "prettierrc",
    "prettier-config",
    "eslint",
    "eslintrc",
    "eslint-config"
  ],
  "homepage": "https://github.com/boehringer-ingelheim/create-bi-config.git",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "index.js",
    "dist",
    "files"
  ],
  "scripts": {
    "dev": "node index.js",
    "dev:prepare": "unbuild --stub",
    "dev:watch": "yarn dev; chokidar 'src/**/*.ts' -c 'yarn dev --retries=0'",
    "build": "unbuild",
    "prettier": "prettier --write --ignore-unknown .",
    "lint": "eslint --fix .",
    "release": "semantic-release",
    "postinstall": "husky install"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@boehringer-ingelheim/eslint-config": "^1.1.3",
    "@boehringer-ingelheim/prettier-config": "^1.0.0",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@commitlint/types": "17.4.4",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/cross-spawn": "^6.0.2",
    "@types/node": "^16.18.28",
    "@types/prompts": "^2.4.4",
    "chokidar": "^3.5.3",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "semantic-release": "^19.0.5",
    "typescript": "^4.9.5",
    "unbuild": "^1.2.1"
  },
  "dependencies": {
    "prompts": "^2.4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/boehringer-ingelheim/create-bi-config.git"
  },
  "bugs": {
    "url": "https://github.com/boehringer-ingelheim/create-bi-config/issues"
  }
}
