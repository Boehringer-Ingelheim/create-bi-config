{
  "name": "create-bi-config",
  "version": "0.0.2",
  "type": "module",
  "license": "MIT",
  "author": "Boehringer Ingelheim",
  "description": "Add shared configs to your project.",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "index.js",
    "dist",
    "files"
  ],
  "scripts": {
    "dev": "node index.js",
    "dev:prepare": "unbuild --stub",
    "dev:watch": "yarn dev; chokidar 'src/**/*.ts' -c 'yarn dev --retries=0'",
    "build": "unbuild",
    "prettier": "prettier --write --ignore-unknown .",
    "lint": "eslint --fix ."
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@boehringer-ingelheim/eslint-config": "^1.1.0",
    "@boehringer-ingelheim/prettier-config": "^1.0.0",
    "@types/cross-spawn": "^6.0.2",
    "@types/node": "^16.14.2",
    "@types/prompts": "^2.4.2",
    "chokidar": "^3.5.3",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2"
  },
  "dependencies": {
    "prompts": "^2.4.2"
  }
}
