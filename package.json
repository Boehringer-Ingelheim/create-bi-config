{
  "name": "create-bi-config",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "author": "Boehringer Ingelheim",
  "description": "Add shared configs to your project.",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "node index.js",
    "dev:prepare": "unbuild --stub",
    "dev:watch": "yarn dev; chokidar 'src/**/*.ts' -c 'yarn dev --retries=0'",
    "build": "unbuild"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/prompts": "^2.4.2",
    "chokidar": "^3.5.3",
    "chokidar-cli": "^3.0.0",
    "unbuild": "^1.1.2"
  },
  "dependencies": {
    "prompts": "^2.4.2"
  }
}
